<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools">
    <ImageView android:id="@+id/toolbar_back"
        android:layout_width="@dimen/dp_74"
        android:layout_height="@dimen/dp_74"
        android:src="@drawable/ic_arrow_back_ios"
        android:layout_gravity="left|center"
        android:layout_marginLeft="@dimen/dp_64"
        android:visibility="visible"/>
    <ImageView android:id="@+id/toolbar_title"
        android:layout_width="@dimen/dp_260"
        android:layout_height="@dimen/dp_61"
        tools:src="@drawable/ic_green_title"
        android:layout_gravity="center"
        android:visibility="visible"/>
    <ImageView android:id="@+id/toolbar_education"
        android:layout_width="@dimen/dp_62"
        android:layout_height="@dimen/dp_62"
        tools:src="@drawable/ic_education"
        android:layout_gravity="right|center"
        android:layout_marginRight="@dimen/dp_64"
        android:visibility="visible"/>
</FrameLayout>